{
  "name": "kutumb",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-dev": "ENVFILE=.env.dev react-native run-android",
    "android-staging": "ENVFILE=.env.staging react-native run-android",
    "android-prod": "ENVFILE=.env.prod react-native run-android",
    "build-android-dev": "cd android && ./gradlew clean && ENVFILE=.env.dev ./gradlew assembleRelease",
    "build-android-staging": "cd android && ./gradlew clean && ENVFILE=.env.staging ./gradlew assembleRelease",
    "build-android-prod": "cd android && ./gradlew clean && ENVFILE=.env.prod ./gradlew assembleRelease",
    "bundle-android-prod": "cd android && ./gradlew clean && ENVFILE=.env.prod ./gradlew bundleRelease",
    "clean-android": "cd android && rm -R .gradle && cd app && rm -R build",
    "ios": "react-native run-ios --simulator=\"iPhone SE (2nd generation)\"",
    "ios-dev": "ENVFILE=.env.dev react-native run-ios --simulator=\"iPhone 13\"",
    "ios-staging": "ENVFILE=.env.staging react-native run-ios --simulator=\"iPhone 13\"",
    "ios-prod": "ENVFILE=.env.prod react-native run-ios --simulator=\"iPhone 13\"",
    "ios11": "react-native run-ios --simulator=\"iPhone 11\"",
    "ios12": "ENVFILE=.env.dev react-native run-ios --simulator=\"iPhone 12 Pro Max\"",
    "ios13mini": "ENVFILE=.env.dev react-native run-ios --simulator=\"iPhone 13 mini\"",
    "iosx": "react-native run-ios --simulator=\"iPhone Xs Max\"",
    "ios8": "react-native run-ios --simulator=\"iPhone 8\"",
    "iosSE": "react-native run-ios --simulator=\"iPhone SE (3rd generation)\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "reconnect": "adb reverse tcp:8081 tcp:8081",
    "menu": "adb shell input keyevent 82"
  },
